    <xsd:annotation>
        <xsd:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:format
                encoding="US-ASCII"
                representation="text"
                alignment="1"
                alignmentUnits="bytes"
                lengthKind="explicit"
                occursCountKind="implicit"
                textNumberRep="standard"
                textStandardDecimalSeparator="."
                textStandardGroupingSeparator=","/>
        </xsd:appinfo>
    </xsd:annotation>

    <!-- ========================================== -->
    <!-- Return detail type (was IPRNT_Ret_Dtl_Type) -->
    <!-- ========================================== -->
    <xsd:complexType name="IPRNT_Ret_Dtl_Type">
        <xsd:sequence>
            <!-- Error code: fixed 6 chars -->
            <xsd:element name="Error_Cde" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="6"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <!-- Error message: fixed 255 chars -->
            <xsd:element name="Error_Msg" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="255"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- ========================================== -->
    <!-- ISIS iPRNT 200 Response type                -->
    <!-- ========================================== -->
    <xsd:complexType name="ISIS_iPRNT_200Res_Type">
        <xsd:sequence>
            <!-- Header imported from IPRINT_Request.dfdl.xsd -->
            <xsd:element name="ISIS_iPRNT_EDX_Header" type="EDX_HDR_Type"/>

            <!-- Communication response from ISIS_iPRNT_300_MsgRes.dfdl.xsd -->
            <xsd:element name="ISIS_iPRNT_Comm" type="ISIS_iPRNT_CommRes_Type"/>

            <!-- Return detail -->
            <xsd:element name="ISIS_iPRINT_Ret_Dtl" type="IPRNT_Ret_Dtl_Type"/>

            <!-- Letter_Mode200: fixed 5 chars -->
            <xsd:element name="Letter_Mode200" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="5"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- ========================================== -->
    <!-- Root element                                -->
    <!-- ========================================== -->
    <xsd:element name="ISIS_iPRINT_200_MSG" type="ISIS_iPRNT_200Res_Type"/>

</xsd:schema>
