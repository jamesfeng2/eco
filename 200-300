<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
    xmlns:fn="http://www.w3.org/2005/xpath-functions"
    targetNamespace="http://iprint_mq.com/ISIS_iPRNT_300Res"
    xmlns="http://iprint_mq.com/ISIS_iPRNT_300Res"
    elementFormDefault="qualified">

    <!-- ========================================== -->
    <!-- Includes (converted to .dfdl.xsd)           -->
    <!-- ========================================== -->
    <xsd:include schemaLocation="IPRINT_Request.dfdl.xsd"/>

    <!-- ========================================== -->
    <!-- Global DFDL format                          -->
    <!-- ========================================== -->
    <xsd:annotation>
        <xsd:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:format
                encoding="US-ASCII"
                representation="text"
                alignment="1"
                alignmentUnits="bytes"
                lengthKind="explicit"
                occursCountKind="implicit"
                textNumberRep="standard"
                textStandardDecimalSeparator="."
                textStandardGroupingSeparator=","/>
        </xsd:appinfo>
    </xsd:annotation>

    <!-- ========================================== -->
    <!-- ISIS iPRNT Communication Response type      -->
    <!-- ========================================== -->
    <xsd:complexType name="ISIS_iPRNT_CommRes_Type">
        <xsd:sequence>
            <!-- TRN_Type_Cde: fixed 6 chars -->
            <xsd:element name="TRN_Type_Cde" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="6"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <!-- CRN: fixed 10 chars -->
            <xsd:element name="CRN" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="10"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <!-- Hist_Id: fixed 10 chars -->
            <xsd:element name="Hist_Id" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="10"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- ========================================== -->
    <!-- ISIS iPRNT 300 Response type                -->
    <!-- ========================================== -->
    <xsd:complexType name="ISIS_iPRNT_300Res">
        <xsd:sequence>
            <!-- Header from IPRINT_Request.dfdl.xsd -->
            <xsd:element name="ISIS_iPRNT_EDX_Header" type="EDX_HDR_Type"/>

            <!-- Communication response -->
            <xsd:element name="ISIS_iPRNT_CommRes" type="ISIS_iPRNT_CommRes_Type"/>

            <!-- Letter_Mde300: fixed 5 chars -->
            <xsd:element name="Letter_Mde300" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="5"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- ========================================== -->
    <!-- Root element                                -->
    <!-- ========================================== -->
    <xsd:element name="ISIS_iPRINT_300_MSG" type="ISIS_iPRNT_300Res"/>

</xsd:schema>

