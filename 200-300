<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
    xmlns:fn="http://www.w3.org/2005/xpath-functions"
    targetNamespace="http://iprint_mq.com/IPRINT_300Res"
    xmlns="http://iprint_mq.com/IPRINT_300Res"
    elementFormDefault="qualified">

    <!-- ========================================== -->
    <!-- Includes (replace .mxsd with .dfdl.xsd)     -->
    <!-- ========================================== -->
    <xsd:include schemaLocation="IPRINT_Request.dfdl.xsd"/>
    <xsd:include schemaLocation="IPRINT_200_MsgRes.dfdl.xsd"/>
    <xsd:include schemaLocation="ISIS_iPRNT_200_MsgRes.dfdl.xsd"/>

    <!-- ========================================== -->
    <!-- Global DFDL format                          -->
    <!-- ========================================== -->
    <xsd:annotation>
        <xsd:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:format
                encoding="US-ASCII"
                representation="text"
                alignment="1"
                alignmentUnits="bytes"
                lengthKind="explicit"
                occursCountKind="implicit"
                textNumberRep="standard"
                textStandardDecimalSeparator="."
                textStandardGroupingSeparator=","/>
        </xsd:appinfo>
    </xsd:annotation>

    <!-- ========================================== -->
    <!-- Main 300Res type                            -->
    <!-- ========================================== -->
    <xsd:complexType name="IPRINT_300Res_Type">
        <xsd:sequence>
            <!-- Header from IPRINT_Request.dfdl.xsd -->
            <xsd:element name="EDX_HDR_300Res" type="EDX_HDR_Type"/>

            <!-- ISIS header from ISIS_iPRNT_200_MsgRes.dfdl.xsd -->
            <xsd:element name="ISIS_Header_300Res" type="ISIS_Header_Type"/>

            <!-- BRK_RQSTData_300 (null-terminated string) -->
            <xsd:element name="BRK_RQSTData_300" type="BRK_RQST_Type">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="delimited"
                            terminator="%NUL;"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <!-- Return detail -->
            <xsd:element name="IPRNT_Ret_Det_300Res" type="IPRNT_Ret_Dtl_Type"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- ========================================== -->
    <!-- Root element                                -->
    <!-- ========================================== -->
    <xsd:element name="IPRINT_300RES_MSG" type="IPRINT_300Res_Type"/>

</xsd:schema>


