<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:include schemaLocation="IPRINT_Request.mxsd"/>
    <xsd:complexType name="ISIS_iPRNT_CommRes_Type">
        <xsd:sequence>
            <xsd:element name="TRN_Type_Cde" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="WMQI_APPINFO">
            <cwfInclRep messageSetDefaultRep="CWF1" repeatCount="1">
              <cwfSimpleRep encodingNull="NullLogicalValue" encodingNullValue="" mrLengthEncoding="fixedLength" paddingCharacter="SPACE" stringJustification="leftJustify" typeName="MRCWFStringRep" width="6"/>
            </cwfInclRep>
          </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CRN" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="WMQI_APPINFO">
            <cwfInclRep messageSetDefaultRep="CWF1">
              <cwfSimpleRep encodingNull="NullLogicalValue" encodingNullValue="" mrLengthEncoding="fixedLength" paddingCharacter="SPACE" stringJustification="leftJustify" typeName="MRCWFStringRep" width="10"/>
            </cwfInclRep>
          </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Hist_Id" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="WMQI_APPINFO">
            <cwfInclRep messageSetDefaultRep="CWF1" repeatCount="1">
              <cwfSimpleRep encodingNull="NullLogicalValue" encodingNullValue="" mrLengthEncoding="fixedLength" paddingCharacter="SPACE" stringJustification="leftJustify" typeName="MRCWFStringRep" width="10"/>
            </cwfInclRep>
          </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ISIS_iPRNT_300Res">
        <xsd:sequence>
            <xsd:element name="ISIS_iPRNT_EDX_Header" type="EDX_HDR_Type"/>
            <xsd:element name="ISIS_iPRNT_CommRes" type="ISIS_iPRNT_CommRes_Type"/>
            <xsd:element name="Letter_Mde300" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="WMQI_APPINFO">
            <cwfInclRep messageSetDefaultRep="CWF1" repeatCount="1">
              <cwfSimpleRep encodingNull="NullLogicalValue" encodingNullValue="" mrLengthEncoding="fixedLength" paddingCharacter="SPACE" stringJustification="leftJustify" typeName="MRCWFStringRep" width="5"/>
            </cwfInclRep>
          </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="ISIS_iPRINT_300_MSG" type="ISIS_iPRNT_300Res">
        <xsd:annotation>
            <xsd:appinfo source="WMQI_APPINFO">
        <MRMessage messageDefinition="/0/ISIS_iPRINT_300_MSG;XSDElementDeclaration$MRObject"/>
      </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
