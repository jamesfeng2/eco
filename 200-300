
IPRINT_200_MsgRes.mxsd

<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:include schemaLocation="ISIS_iPRNT_200_MsgRes.mxsd"/>
    <xsd:include schemaLocation="IPRINT_Request.mxsd"/>
    <xsd:complexType name="IPRINT_200Res_Type">
        <xsd:sequence>
            <xsd:element name="EDX_HDR_200Res" type="EDX_HDR_Type"/>
            <xsd:element name="ISIS_Header_200Res" type="ISIS_Header_Type"/>
            <xsd:element name="BRK_RQSTData_200Res" type="BRK_RQST_Type">
                <xsd:annotation>
                    <xsd:appinfo source="WMQI_APPINFO">
            <cwfInclRep messageSetDefaultRep="CWF1">
              <cwfSimpleRep encodingNull="NullLogicalValue" encodingNullValue="" mrLengthEncoding="nullTerminated" paddingCharacter="NUL" stringJustification="leftJustify" typeName="MRCWFStringRep"/>
            </cwfInclRep>
          </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IPRINT_Return_200Res" type="IPRNT_Ret_Dtl_Type"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="IPRINT_Return_Type">
        <xsd:sequence>
            <xsd:element name="Return_Cde" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="WMQI_APPINFO">
            <cwfInclRep messageSetDefaultRep="CWF1" repeatCount="1">
              <cwfSimpleRep encodingNull="NullLogicalValue" encodingNullValue="" mrLengthEncoding="fixedLength" paddingCharacter="SPACE" stringJustification="leftJustify" typeName="MRCWFStringRep" width="6"/>
            </cwfInclRep>
          </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Return_Msg" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="WMQI_APPINFO">
            <cwfInclRep messageSetDefaultRep="CWF1" repeatCount="1">
              <cwfSimpleRep encodingNull="NullLogicalValue" encodingNullValue="" mrLengthEncoding="nullTerminated" paddingCharacter="SPACE" stringJustification="leftJustify" typeName="MRCWFStringRep"/>
            </cwfInclRep>
          </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="IPRINT_200RES_MSG" type="IPRINT_200Res_Type">
        <xsd:annotation>
            <xsd:appinfo source="WMQI_APPINFO">
        <MRMessage messageDefinition="/0/IPRINT_200RES_MSG;XSDElementDeclaration$MRObject"/>
      </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
</xsd:schema>

------------
<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
    xmlns:fn="http://www.w3.org/2005/xpath-functions"
    targetNamespace="http://iprint_mq.com/IPRINT_200Res"
    xmlns="http://iprint_mq.com/IPRINT_200Res"
    elementFormDefault="qualified">

    <!-- ========================================== -->
    <!-- Includes for referenced types               -->
    <!-- ========================================== -->
    <xsd:include schemaLocation="ISIS_iPRNT_200_MsgRes.dfdl.xsd"/>
    <xsd:include schemaLocation="IPRINT_Request.dfdl.xsd"/>

    <!-- ========================================== -->
    <!-- Global format                               -->
    <!-- ========================================== -->
    <xsd:annotation>
        <xsd:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:format
                encoding="US-ASCII"
                representation="text"
                alignment="1"
                alignmentUnits="bytes"
                lengthKind="explicit"
                occursCountKind="implicit"
                textNumberRep="standard"
                textStandardDecimalSeparator="."
                textStandardGroupingSeparator=","/>
        </xsd:appinfo>
    </xsd:annotation>

    <!-- ========================================== -->
    <!-- Return detail type (was IPRINT_Return_Type) -->
    <!-- ========================================== -->
    <xsd:complexType name="IPRINT_Return_Type">
        <xsd:sequence>
            <!-- Fixed length 6 chars -->
            <xsd:element name="Return_Cde" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="explicit"
                            length="6"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <!-- Null-terminated string (simulated as explicit length OR terminator) -->
            <xsd:element name="Return_Msg" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <dfdl:format
                            lengthKind="delimited"
                            terminator="%NUL;"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- ========================================== -->
    <!-- Main 200Res type                            -->
    <!-- ========================================== -->
    <xsd:complexType name="IPRINT_200Res_Type">
        <xsd:sequence>
            <xsd:element name="EDX_HDR_200Res" type="EDX_HDR_Type"/>
            <xsd:element name="ISIS_Header_200Res" type="ISIS_Header_Type"/>

            <!-- BRK_RQSTData_200Res -->
            <xsd:element name="BRK_RQSTData_200Res" type="BRK_RQST_Type">
                <xsd:annotation>
                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                        <!-- simulate null-terminated string -->
                        <dfdl:format
                            lengthKind="delimited"
                            terminator="%NUL;"
                            representation="text"
                            textJustification="left"
                            textPadCharacter=" "/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <!-- Return detail -->
            <xsd:element name="IPRINT_Return_200Res" type="IPRINT_Return_Type"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- ========================================== -->
    <!-- Root element                                -->
    <!-- ========================================== -->
    <xsd:element name="IPRINT_200RES_MSG" type="IPRINT_200Res_Type"/>

</xsd:schema>

