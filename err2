<!-- Repeated insert details, count controlled by ISIS_Insert_Cnt -->
<xsd:element name="ISIS_Insert_Dtl"
             type="ISIS_Insert_Dtl_Type"
             minOccurs="0"
             maxOccurs="unbounded">
  <xsd:annotation>
    <xsd:appinfo source="http://www.ogf.org/dfdl/">
      <!-- Repetition count comes from ISIS_Insert_Cnt under ISIS_Header_Type -->
      <dfdl:element
          dfdl:occursCountKind="expression"
          dfdl:occursCount="{ ../ISIS_Header_Type/ISIS_Insert_Cnt }"
          dfdl:separator="none" />
    </xsd:appinfo>
  </xsd:annotation>
</xsd:element>

<!-- Each insert record -->
<xsd:complexType name="ISIS_Insert_Dtl_Type">
  <xsd:sequence dfdl:separator="none" dfdl:initiatedContent="no">
    <!-- insertCode -->
    <xsd:element name="insertCode" type="xsd:string"
                 dfdl:length="10"
                 dfdl:lengthKind="explicit"
                 dfdl:representation="text"
                 dfdl:textPadKind="padChar"
                 dfdl:textPadCharacter=" "
                 dfdl:textJustification="left" />

    <!-- insertName -->
    <xsd:element name="insertName" type="xsd:string"
                 dfdl:length="50"
                 dfdl:lengthKind="explicit"
                 dfdl:representation="text"
                 dfdl:textPadKind="padChar"
                 dfdl:textPadCharacter=" "
                 dfdl:textJustification="left" />
  </xsd:sequence>
</xsd:complexType>
