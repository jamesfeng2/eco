<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
            targetNamespace="http://iprint_mq.com"
            xmlns:tns="http://iprint_mq.com"
            elementFormDefault="qualified">

  <!-- Global format -->
  <xsd:annotation>
    <xsd:appinfo source="http://www.ogf.org/dfdl/">
      <dfdl:format representation="text"
                   encoding="ISO-8859-1"
                   lengthKind="explicit"
                   textPadKind="padRight"
                   textPadCharacter=" "
                   alignment="1"
                   alignmentUnits="bytes"
                   outputNewLine="%CR;%LF;"/>
    </xsd:appinfo>
  </xsd:annotation>

  <!-- ===================== -->
  <!--   EDX_HDR_Type        -->
  <!-- ===================== -->
  <xsd:complexType name="EDX_HDR_Type">
    <xsd:sequence dfdl:separator="">
      <xsd:element name="EDX_Header_Type" type="xsd:string"
                   dfdl:length="1"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"/>

      <xsd:element name="EDX_Header_Ver" type="xsd:int"
                   dfdl:length="1"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"
                   dfdl:textNumberRep="standard"/>

      <xsd:element name="EDX_TRN_Type_Cde" type="xsd:string"
                   dfdl:length="10"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"/>

      <xsd:element name="EDX_TRN_Action_Cde" type="xsd:string"
                   dfdl:length="1"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"/>

      <xsd:element name="EDX_TRN_Ver_Num" type="xsd:int"
                   dfdl:length="2"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"
                   dfdl:textNumberRep="standard"/>

      <!-- replicate for all other fields with widths from your MRM schema -->

      <xsd:element name="EDX_Sent_Date" type="xsd:date"
                   dfdl:length="8"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"
                   dfdl:calendarPattern="yyyyMMdd"/>

      <xsd:element name="EDX_Sent_Time_Seq_Num" type="xsd:string"
                   dfdl:length="12"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"/>

      <xsd:element name="EDX_Data_Len" type="xsd:int"
                   dfdl:length="6"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"
                   dfdl:textNumberRep="standard"
                   dfdl:textNumberPadCharacter="0"
                   dfdl:textNumberJustification="right"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- ===================== -->
  <!--   ISIS_Header_Type    -->
  <!-- ===================== -->
  <xsd:complexType name="ISIS_Header_Type">
    <xsd:sequence dfdl:separator="">
      <xsd:element name="ISIS_TRN_Type_Cde" type="xsd:string"
                   dfdl:length="6"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"/>
      <!-- replicate rest (CRN, Host_Id, etc.) using same pattern -->
    </xsd:sequence>
  </xsd:complexType>

  <!-- BRK_RQST -->
  <xsd:complexType name="BRK_RQST_Type">
    <xsd:sequence>
      <xsd:element name="BRK_RQST_Metadata" type="xsd:string"
                   dfdl:length="128"
                   dfdl:lengthKind="explicit"
                   dfdl:representation="text"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- ISIS_PDF_PRNT_Dtl -->
  <xsd:complexType name="ISIS_PDF_PRNT_Dtl_Type">
    <xsd:sequence>
      <xsd:element name="ISIS_Printer_PRNT_Dtl" type="xsd:string"
                   dfdl:lengthKind="delimited"
                   dfdl:representation="text"
                   dfdl:occursCountKind="implicit"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- Root -->
  <xsd:element name="IPRINT_REQ_MSG" type="tns:IPRINT_REQ_Type"/>
  <xsd:complexType name="IPRINT_REQ_Type">
    <xsd:sequence>
      <xsd:element name="EDX_HDR" type="tns:EDX_HDR_Type"/>
      <xsd:element name="ISIS_Header" type="tns:ISIS_Header_Type"/>
      <xsd:element name="BRK_RQST" type="tns:BRK_RQST_Type"/>
      <xsd:element name="ISIS_PDF_PRNT_Dtl" type="tns:ISIS_PDF_PRNT_Dtl_Type"/>
    </xsd:sequence>
  </xsd:complexType>

